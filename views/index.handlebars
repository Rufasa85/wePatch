{{#each patches}}
    <div class="card">
        <h1>{{User.name}}'s patch</h1>
        <h3>lat :{{lat}} lng: {{lng}} area: {{area}}</h3>
        {{#if GardenerId}}
        <h5>Gardener: {{GardenerId}}</h5>
        {{/if}}
        {{#unless GardenerId}}
        <button class="claimPatch" data-id={{id}}>claim patch!</button>
        {{/unless}}
    </div>

{{/each}}
<script src="/js/home.js"></script>